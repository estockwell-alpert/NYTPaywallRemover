{

  "manifest_version": 2,
  "name": "NYT Paywall Remover",
  "version": "1.0",

  "description": "Removes the paywall on nytimes.com",
  "homepage_url": "https://github.com/estockwell-alpert/NYTPaywallRemover",
  "icons": {
    "48": "icons/koolaid_48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/koolaid_32.png",
    "default_title": "NYT Paywall Remover",
    "default_popup": "popup/choose_beast.html"
  },

  "web_accessible_resources": [
    "beasts/koolaid_128.png"
  ]

}